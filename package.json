{
    "name": "rollup-ts-basic-template-cli",
    "version": "1.0.0",
    "description": "rollup-ts-basic-template-cli",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "index.d.ts",
    "type": "module",
    "bin": {
        "parrot": "./dist/index.js"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install",
        "build": "rollup -c && npm run doc",
        "build:tsc": "npx tsc",
        "dev": "rollup -c --watch",
        "lint": "eslint --ext .ts src --config=.eslintrc.json",
        "lint:fix": "eslint --ext .ts src --fix --config=.eslintrc.json",
        "lint-staged": "lint-staged",
        "commitlint": "commitlint --config commitlint.config.cjs -e -V"
    },
    "lint-staged": {
        "src/**/*.{ts}": "npm run lint"
    },
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.common.js",
            "types": "./index.d.ts"
        }
    },
    "files": [
        "dist",
        "*.d.ts"
    ],
    "keywords": [
        "typescript",
        "javascript"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/supuwoerc/rollup-ts-basic-template-cli.git"
    },
    "author": "QiMeng Zhang <zhangzhouou@gmail.com>(https://github.com/supuwoerc)",
    "homepage": "https://supuwoerc.github.io/rollup-ts-basic-template-cli",
    "bugs": {
        "url": "https://github.com/supuwoerc/rollup-ts-basic-template-cli/issues"
    },
    "license": "MIT",
    "dependencies": {
        "chalk": "^4.1.1",
        "clear": "^0.1.0",
        "commander": "^11.0.0",
        "download-git-repo": "^3.0.2",
        "ejs": "^3.1.9",
        "figlet": "^1.6.0",
        "git-clone": "^0.2.0",
        "glob": "^10.3.3",
        "handlebars": "^4.7.7",
        "inquirer": "^9.2.8",
        "latest-version": "^7.0.0",
        "log-symbols": "^5.1.0",
        "metalsmith": "^2.6.1",
        "minimatch": "^9.0.3",
        "ora": "^6.3.1",
        "rimraf": "^5.0.1",
        "wrap-ansi": "^8.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.21.8",
        "@babel/preset-env": "^7.22.9",
        "@commitlint/cli": "^17.6.3",
        "@commitlint/config-conventional": "^17.6.3",
        "@rollup/plugin-alias": "^5.0.0",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^24.1.0",
        "@rollup/plugin-eslint": "^9.0.3",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@types/chalk": "^2.2.0",
        "@types/commander": "^2.12.2",
        "@types/inquirer": "^9.0.3",
        "@types/jest": "^29.5.1",
        "@types/node": "^20.4.4",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "add": "^2.0.6",
        "esbuild": "^0.17.18",
        "eslint": "^8.40.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-custom-alias": "^1.3.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "lint-staged": "^13.2.2",
        "prettier": "^2.8.8",
        "rollup": "^3.21.5",
        "rollup-plugin-cleaner": "^1.0.0",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-dts": "^5.3.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.34.1",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.4"
    },
    "volta": {
        "node": "16.14.2"
    }
}
